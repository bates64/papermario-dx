{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Mod",
      "type": "shell",
      "command": "nix --extra-experimental-features nix-command develop --accept-flake-config --command ./tools/run",
      "problemMatcher": "$gcc",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Open Star Rod",
      "type": "shell",
      "command": "nix --extra-experimental-features nix-command develop --accept-flake-config --command star-rod",
      "problemMatcher": [],
      "group": {
        "kind": "none"
      },
    }
  ]
}
