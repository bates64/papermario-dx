{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Mod",
      "type": "shell",
      "command": "nix develop --profile .nix-profile --command ./run",
      "problemMatcher": "$gcc",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Open Star Rod",
      "type": "shell",
      "command": "nix develop --profile .nix-profile --command star-rod",
      "problemMatcher": [],
      "group": {
        "kind": "none"
      },
    }
  ]
}
